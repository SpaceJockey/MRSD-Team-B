<launch>
	<include file="$(find spacejockey)/launch/core.launch" />
	<include file="$(find spacejockey)/launch/ar_tags.launch" />
	<node name="minor_planner" pkg="spacejockey" type="minor_planner.py" />
	<node pkg="spacejockey" type="control_bridge.py" name="ctl_bridge" />
	<!-- one of these will always fail -->
	<node name="serial_node_XBee" pkg="rosserial_python" type="serial_node.py" args="/dev/ttyUSB0" />
	<node name="serial_node_USB" pkg="rosserial_python" type="serial_node.py" args="/dev/ttyACM0" />

	<node name="world_warp" pkg="spacejockey" type="world_warp.py" />
	<node name="waypoint_planner" pkg="spacejockey" type="waypoint_planner.py" required="true" />
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find spacejockey)/resources/spacejockey.rviz" required="true" />
</launch>
